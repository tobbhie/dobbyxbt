{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.9"
    },
    "api/setup.py": {
      "runtime": "python3.9"
    },
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/setup",
      "dest": "/api/setup.py"
    },
    {
      "src": "/api/index",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram-bot-token",
    "CRYPTORANK_API_KEY": "@cryptorank-api-key",
    "MODEL_API_KEY": "@model-api-key"
  },
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}
