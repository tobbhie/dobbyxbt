{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.12"
    },
    "api/setup.py": {
      "runtime": "python3.12"
    },
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/setup",
      "dest": "/api/setup.py"
    },
    {
      "src": "/api/index",
      "dest": "/api/index.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}
